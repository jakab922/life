body {
  background: url(../images/bg.jpg) #2a2a2a repeat-x top center;
  font-family: Helvetica, Arial, sans-serif;
  font-size: 62.5%;
  color: #1b191a;
  line-height: 1.6em;
}
a {
  color: #1b191a;
  text-decoration: none;
}
a:hover {
  color: #f89829;
}
#container {
  font-size: 1.2em;
  overflow: hidden;
  position: relative;
}

input[type="submit"] {
	-webkit-appearance: none;
}

#topbar {
  width: 100%;
  position: relative;
  overflow: hidden;
  background: #ffffff;
  padding-bottom: 10px;
  padding-top: 15px;
}
#topbar_inner {
  width: 966px;
  margin: 0 auto;
}
#topbar_1, #topbar_2, #topbar_3 {
  float: left;
}
#topbar_1 {
  width: 139px;
  margin-left: 18px;
}
#topbar_2 {
  width: 564px;
  border-left: 1px solid #777777;
  padding-bottom: 10px;
  margin-top: 10px;
}
#topbar_2 h2 {
  display: block;
  font-size: 2.6em;
  margin-left: 20px;
  margin-top: 50px;
  font-weight: bold;
  line-height: 1.1em;
}
#topbar_2 h2 span {
  font-weight: normal;
}
.landlords.services #topbar_2 h2 span {
  display: block;
  font-size: 0.8em;
  padding-left: 1px;
}
.search #topbar_2 h2, .detail #topbar_2 h2 {
  margin-top: 20px;
  font-size: 2em;
}
.search #topbar_2 p, .detail #topbar_2 p {
  color: #777777;
  margin-top: 20px;
  margin-left: 20px;
  font-size: 1.2em;
}
#topbar_3 {
  margin-top: 15px;
}
#topbar ul#nav {
  width: 474px;
  margin-left: 20px;
  font-size: 1.1em;
}
ul.nav_mobile, .topnav_mobile {
  display: none;
  /* hide mobile navs */

}
#topbar strong {
  font-weight: bold;
}
#topbar #language {
  overflow: hidden;
  font-size: 0.9em;
  color: #777777;
  margin-top: 20px;
}
#topbar #language img {
  margin-left: 10px;
  position: relative;
  top: 2px;
}
#topbar #shortlist-link {
  font-weight: bold;
  font-size: 0.9em;
  margin-top: 10px;
}
#topbar ul#nav li {
  display: inline;
  border-right: 1px solid #999999;
  padding-right: 7px;
  padding-left: 3px;
}
#topbar ul#nav li:last-child {
  border: none;
}
#mainbody {
  width: 966px;
  margin: 0 auto;
  background: #ffffff;
  padding-bottom: 20px;
  position: relative;
  overflow: hidden;
}
.page #mainbody {
  margin-top: 28px;
}
#mainbody p {
  color: #777777;
}
#banner {
  position: relative;
}
#feature {
  position: relative;
}
ol.carousel-tabs {
  overflow: hidden;
  position: absolute;
  bottom: 20px;
  left: 48%;
}
ol.carousel-tabs li {
  display: block;
  float: left;
  margin-left: 1px;
}
ol.carousel-tabs li a {
  display: block;
  width: 15px;
  height: 15px;
  background: url(../images/unselected_control.png) no-repeat;
  text-indent: -99999px;
}
ol.carousel-tabs li.carousel-active-tab a {
  background-image: url(../images/selected_control.png);
}

ul.slidecontrols {
	width: 690px;
	padding-left: 3px;
	background: transparent;
	overflow:hidden;
	height: 53px;
	position: absolute;
	top: 45%;
	margin: 0 auto;
}

.home ul.slidecontrols {
	display:none;
}

ul.slidecontrols li {
	display:block;
	float: left;
}

ul.slidecontrols li:nth-child(2) {
	float:right;
}

ul.slidecontrols li a {
	display:block;
	height: 53px;
	width: 29px;
	text-indent: -9999px;
	background: url(../images/rightarrow.png) no-repeat;
}

ul.slidecontrols li:first-child a {
	background: url(../images/leftarrow.png) no-repeat;
}

